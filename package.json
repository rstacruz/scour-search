{
  "name": "scour-search",
  "version": "1.0.0",
  "description": "Fast searching",
  "main": "scour-search.node.js",
  "scripts": {
    "test": "mocha",
    "build": "browserify --bare -s scour-search scour-search.js -o scour-search.min.js",
    "build:node": "browserify --bare --node --no-bundle-external scour-search.js -o scour-search.node.js",
    "watch": "watchify -v --bare -s scour-search scour-search.js -o scour-search.min.js",
    "prepublish": "npm run build & npm run build:node & wait"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "6.3.26",
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "benchmark": "1.0.0",
    "browserify": "12.0.1",
    "expect": "1.13.4",
    "mocha": "2.3.4",
    "sift": "3.0.0",
    "watchify": "3.6.1"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "babel-core": "6.3.26",
    "expect": "^1.13.4",
    "object-assign": "4.0.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
