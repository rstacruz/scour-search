{
  "name": "scour-search",
  "description": "Fast searching",
  "version": "0.2.0",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com>",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "bugs": {
    "url": "https://github.com/rstacruz/scour-search/issues"
  },
  "dependencies": {
    "object-assign": "4.0.1"
  },
  "devDependencies": {
    "babel": "6.3.26",
    "babel-core": "6.3.26",
    "babel-preset-es2015": "6.3.13",
    "babelify": "7.2.0",
    "benchmark": "1.0.0",
    "browserify": "12.0.1",
    "expect": "1.13.4",
    "mdx": "0.3.1",
    "mocha": "2.3.4",
    "sift": "3.0.0",
    "watchify": "3.6.1"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/rstacruz/scour-search#readme",
  "keywords": [
    "search"
  ],
  "license": "MIT",
  "main": "scour-search.node.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rstacruz/scour-search.git"
  },
  "scripts": {
    "build": "browserify --bare -s scour-search scour-search.js -o scour-search.min.js",
    "build:node": "browserify --bare -s scour-search --node --no-bundle-external scour-search.js -o scour-search.node.js",
    "prepublish": "npm run build & npm run build:node & wait",
    "test": "mocha",
    "watch": "watchify -v --bare -s scour-search scour-search.js -o scour-search.min.js"
  }
}
